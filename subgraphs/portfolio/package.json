{
	"name": "dankbank-subgraph",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"codegen": "graph codegen",
		"build": "graph build",
		"deploy:mainnet": "yarn prepare:mainnet && graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ dankbanklabs/dank-bank-mainnet",
		"deploy:goerli": "yarn prepare:goerli && graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ dankbanklabs/dank-bank-goerli",
		"create-local": "graph create --node http://localhost:8020/ DankBankLabs/dank-graph",
		"remove-local": "graph remove --node http://localhost:8020/ DankBankLabs/dank-graph",
		"deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 DankBankLabs/dank-graph",
		"prepare:mainnet": "mustache templates/mainnet.json subgraph.template.yaml > subgraph.yaml",
		"prepare:goerli": "mustache templates/goerli.json subgraph.template.yaml > subgraph.yaml",
		"prepare:localhost": "mustache templates/localhost.json subgraph.template.yaml > subgraph.yaml",
		"prepare:maticLocalhost": "mustache templates/maticLocalhost.json maticSubgraph.template.yaml > subgraph.yaml",
		"prepare:matic": "mustache templates/matic.json maticSubgraph.template.yaml > subgraph.yaml",
		"prepare:mumbai": "mustache templates/mumbai.json maticSubgraph.template.yaml > subgraph.yaml",
		"test": "hardhat test",
		"typechain": "hardhat typechain"
	},
	"keywords": [],
